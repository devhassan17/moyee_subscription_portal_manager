<odoo>
    <template id="report_invoice_document_inherit_moyee"
              inherit_id="account.report_invoice_document">

        <xpath expr="//t[@t-foreach='o.invoice_line_ids']" position="attributes">
            <attribute name="t-foreach">
                o.invoice_line_ids.filtered(lambda l: l.quantity &gt; 0)
            </attribute>
        </xpath>

    </template>
</odoo>